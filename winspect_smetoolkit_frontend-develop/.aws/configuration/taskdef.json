{
  "family": "WINSPECT-SMETOOLKIT-FND-PROD-TASK",
  "containerDefinitions": [
    {
      "name": "winspect-smetoolkit-frontend-prod",
      "image": "<IMAGE1_NAME>",
      "cpu": 0,
      "portMappings": [
        {
          "name": "winspect-smetoolkit-frontend-prod-3000-tcp",
          "containerPort": 3000,
          "hostPort": 3000,
          "protocol": "tcp",
          "appProtocol": "http"
        }
      ],
      "essential": true,
      "environment": [],
      "environmentFiles": [],
      "mountPoints": [],
      "volumesFrom": [],
      "ulimits": [],
      "logConfiguration": {
        "logDriver": "awslogs",
        "options": {
          "awslogs-create-group": "true",
          "awslogs-group": "/ecs/WINSPECT-SMETOOLKIT-FND-PROD-TASK",
          "awslogs-region": "us-east-1",
          "awslogs-stream-prefix": "ecs"
        }
      },
      "healthCheck": {
        "command": [
          "CMD-SHELL",
          "curl --head -f 'http://localhost:3000' || exit 1"
        ],
        "interval": 5,
        "timeout": 10,
        "retries": 5,
        "startPeriod": 180
      }
    }
  ],
  "taskRoleArn": "arn:aws:iam::699630347910:role/ecsTaskExecutionRole",
  "executionRoleArn": "arn:aws:iam::699630347910:role/ecsTaskExecutionRole",
  "networkMode": "awsvpc",
  "requiresCompatibilities": ["FARGATE"],
  "cpu": "1024",
  "memory": "3072",
  "runtimePlatform": {
    "cpuArchitecture": "X86_64",
    "operatingSystemFamily": "LINUX"
  }
}
